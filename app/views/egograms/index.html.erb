<div class="container">
  <div class="row">
    <div class="col-12 text-center">
      <% @egograms.each do |egogram| %>
        <%= link_to egogram_path(egogram.id) do %>
          <%= attachment_image_tag egogram, :egogram_image, size: "300x300" %>
        <% end %>
      <% end %>
      <%= paginate @egograms %>
    </div>
  </div>
</div>